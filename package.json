{
  "name": "my-site",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://xaviersalazar.github.io/",
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "framer-motion": "^3.1.1",
    "gh-pages": "^3.1.0",
    "gsap": "^3.5.1",
    "husky": "^4.3.5",
    "lint-staged": "^10.5.3",
    "node-sass": "4.14.1",
    "prettier": "^2.2.1",
    "react": "^17.0.1",
    "react-bulma-components": "^3.4.0",
    "react-dom": "^17.0.1",
    "react-gsap": "^2.2.1",
    "react-intersection-observer": "^8.31.0",
    "react-scripts": "4.0.1",
    "react-scroll-parallax": "^2.3.5",
    "react-scrollmagic": "^2.2.0",
    "react-tsparticles": "^1.18.11",
    "web-vitals": "^0.2.4"
